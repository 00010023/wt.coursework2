  "license": "GPL-3.0",
  "scripts": {
    "server": "./scripts/server.sh",
    "client": "./scripts/client.sh",
    "docs": "./scripts/docs.sh",
    "format": "prettier --write --check \"**/*.js\" \"**/*.jsx\" \"**/*.md\" \"**/*.css\" \"**/*.vue\"\n",
    "format:check": "prettier --check \"**/*.js\" \"**/*.jsx\" \"**/*.md\" \"**/*.css\" \"**/*.vue\"\n",
    "git": "yarn run dir && ./update",
    "initialize": "./install",
    "all": "./serverland",
    "dir": "[ -f ./tree.txt ] && rm -rf ./tree.txt && tree -a -s -h -D -I 'node_modules|.next|yarn-error.log|dist|.git|.DS_Store|.idea' -o tree.txt"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1"
  }
}
